# Ignore Composer lock file
/composer.lock

# Ignore directories generated by Composer
/drush/contrib
/vendor
/web/core
/web/modules/contrib
/web/themes/contrib
/web/libraries

# Ignore Drupal's file directory
web/sites/*/files

#Ignore instance specific dirs
/config/*
!/config/.gitkeep
/subsite/*
!/subsite/.gitkeep
/web/profiles/*
!/web/profiles/.gitkeep
/web/sites/*
!/web/sites/default
!/web/sites/development.services.yml
!/web/sites/example.settings.php
!/web/sites/example.sites.php
!/web/sites/settings.allsites.php
!/web/sites/sites.php

# Ignore local files
*.local.php
